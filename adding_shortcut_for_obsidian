#Create a appimagename.desktop file in /home/username/.local/share/applications directory
#Copy following inside the document

[Desktop Entry]
Name=Obsidian
Exec=/home/murad/Downloads/Obsidian-0.11.13.AppImage %u
Terminal=false
Type=Application
Icon=/home/murad/Pictures/obsidian.png
StartupWMClass=obsidian
X-AppImage-Version=0.11.13
Comment=Obsidian
Categories=Office;
MimeType=text/html;x-scheme-handler/obsidian;
